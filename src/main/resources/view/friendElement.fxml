<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Label?>

<AnchorPane prefHeight="50.0" prefWidth="286.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FriendElementController" stylesheets="/stylesheets/elements.css">
    <Button fx:id="delete" layoutX="223.0" layoutY="12.0" mnemonicParsing="false" onAction="#delete" text="Delete"
            AnchorPane.rightAnchor="10"/>
    <Label fx:id="from" layoutX="10.0" layoutY="15.0" text="User Name (email)" AnchorPane.leftAnchor="10"/>
    <Button fx:id="message" layoutX="146.0" layoutY="12.0" mnemonicParsing="false" onAction="#message" text="Message"
            AnchorPane.rightAnchor="70"/>
    <CheckBox fx:id="check" layoutX="124.0" layoutY="16.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="0.0"
              visible="false" AnchorPane.rightAnchor="150"/>
</AnchorPane>
