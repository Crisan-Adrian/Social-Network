<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<fx:root type="javafx.scene.layout.AnchorPane" prefHeight="49.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" stylesheets="/stylesheets/elements.css">
    <Button fx:id="accept" layoutX="155.0" layoutY="12.0" mnemonicParsing="false" text="Accept" onAction="#accept"
            AnchorPane.rightAnchor="70"/>
    <Button fx:id="reject" layoutX="217.0" layoutY="12.0" mnemonicParsing="false" text="Reject" onAction="#reject"
            AnchorPane.rightAnchor="10"/>
    <Label fx:id="from" layoutX="10.0" layoutY="10.0" text="User Name (email)" AnchorPane.leftAnchor="10"/>
</fx:root>
